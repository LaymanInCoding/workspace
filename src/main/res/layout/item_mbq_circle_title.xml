<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_marginTop="@dimen/dimen_10_dip"
    android:layout_height="wrap_content">
    <View
        style="@style/mbq_horizontal_divider_full" />
    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_marginLeft="@dimen/dimen_20_dip"
        android:layout_marginTop="@dimen/dimen_10_dip"
        android:layout_marginBottom="@dimen/dimen_10_dip"
        android:layout_height="wrap_content"
        android:textColor="@color/mbq_circle_title"
        android:textSize="@dimen/dimen_28_dip"/>
    <View
        style="@style/mbq_horizontal_divider_full" />
</LinearLayout>